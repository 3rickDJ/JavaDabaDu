class Avion extends Aereo{
-dimensiones string
-peso string
-capacidad int
-velocidad float
-aceleracion float
-marca string
-material string
+encender()
+apagar()
+despegar()
+aterrizar()
+avanzar()
+acelerar()
+detener()
+modificarLatitud(coordinates:float)
+modificarLongitud(coordinates:float)

}
class Helicoptero extends Aereo{
-dimensiones string
-peso string
-capacidad int
-velocidad float
-aceleracion float
-marca string
-material string
+encender()
+apagar()
+despegar()
+aterrizar()
+avanzar()
+acelerar()
+detener()
+cambiarAlaDelta(veveveve)
+modificarLatitud(coordinates:float)
+modificarLongitud(coordinates:float)
}
class Moto extends Terrestre{
-color string
-marca string
-modelo string
-placa int
-velocidad float
-aceleracion float
+encender()
+apagar()
+acelerar()
+detener()
+avanzar()
}
class Caballo extends Terrestre{
-color string
-tamagno string
-peso float
-edad int
-raza string
+relinchar()
+trotar()
+detener()
+correr()
}
class Lancha extends Terrestre{
-color string
-marca string
-modelo string
-placa int
-agno int
