@staruml
abstract MedioDeTransporte{
-capacidad int
-marca string
-dimensiones string
-material string
+encender()
+apagar()
+avanzar()
+acelerar()
+detener()
}
class Vehiculo extends MedioDeTransporte{
-capacidad int
-marca string
-dimensiones string
-material string
+encender()
+apagar()
+avanzar()
+acelerar()
+detener()
}
class Terrestre extends Vehiculo{
-dimensiones string
-peso string
-capacidad int
-velocidad float
-aceleracion float
-marca string
-material string
+encender()
+apagar()
+avanzar()
+acelerar()
+detener()
}
class Aereo extends Vehiculo{
-dimensiones string
-peso string
-capacidad int
-velocidad float
-aceleracion float
-marca string
-material string
+encender()
+apagar()
+despegar()
+aterrizar()
+avanzar()
+acelerar()
+detener()
}
class Piloto{
-sexo boolean
-nombre string
-edad int
-licencia boolean
+conducir()
+dejarDeConducir()
}
